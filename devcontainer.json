{
    "name": "C",
    "image": "piot/codespaces-c-2d-game:0.0.9",
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt",
        "seccomp=unconfined"
    ],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        "ms-vscode.cpptools"
    ],
    "postCreateCommand": "deps fetch --mode clone",
    "remoteUser": "vscode"
}